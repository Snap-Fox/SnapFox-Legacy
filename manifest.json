{
  "name": "SnapFox",
  "description": "dark mode for snap.berkely.edu",
  "version": "0.6.0",
  "version_name": "0.6.0-alpha",
  "manifest_version": 3,
  "background": {
    "service_worker": "/background.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://snap.berkeley.edu/*"],
      "js": ["script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["style.css"],
      "matches": ["https://snap.berkeley.edu/*"],
      "use_dynamic_url": true
    }
  ]
}
